{
  "expo": {
    "name": "Project Zinova Kitchen Assistant",
    "slug": "zinova-food-recipe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Zinova_logo.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/Zinova_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#8B4513"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lobsangtseten.zinova",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Zinova_logo.png",
        "backgroundColor": "#8B4513"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.lobsangtseten.zinova"
    },
    "web": {
      "favicon": "./public/Zinova_logo.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "scheme": "zinova",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    }
  }
}
